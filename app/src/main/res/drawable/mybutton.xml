<?xml version="1.0" encoding="utf-8"?>

<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#fff">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="2dip"
                android:color="#fff" />
            <gradient
                android:angle="90"
                android:endColor="#f8be01"
                android:startColor="#f8be01"
                android:type="linear" />
            <corners android:radius="25dp" />
        </shape>
    </item>
</ripple>